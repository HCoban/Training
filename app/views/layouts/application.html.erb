<!DOCTYPE html>
<html>
<head>
  <title>MusicApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<ul>
  <% if flash[:errors] %>
    <% flash[:errors].each do |error| %>
    <li>"<%= error%>"</li>
    <% end %>
  <% end %>
</ul>

<%= yield %>

</body>
</html>
