<h2>Login Here</h2>

<form action="<%= session_url %>" method="post">
  <%= insert_authenticity_token %>
  Username<input type="text" name="session[username]" value="">
  Password<input type="text" name="session[password]" value="">
  <input type="submit" value="Login">
</form>

<%= button_to "Dude, let me sign up!", new_user_path, method: :get %>
