<h2>Login Here</h2>

<form action="<%= session_url %>" method="post">
  <%= insert_authenticity_token %>
  username <input type="text" name="session[username]" value="">
  password <input type="text" name="session[password]" value="">
  <input type="submit" value="Login">
</form>

<%= link_to "Dude, let me first sign up!", new_user_path, method: :get %>
